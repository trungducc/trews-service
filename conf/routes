# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
POST    /account/sign_up            controllers.AccountController.signUp(request: Request)
POST    /account/sign_in            controllers.AccountController.signIn(request: Request)

POST    /trews/create               controllers.TrewsController.create(request: Request)
GET     /trews/list                 controllers.TrewsController.list(request: Request)

POST    /trews/react                controllers.TrewsController.react(request: Request)
